{
  "name": "neulabscom/laravel-nova-excel",
  "description": "Supercharged Excel exports for Laravel Nova Resources",
  "license": "MIT",
  "keywords": [
    "laravel",
    "phpspreadsheet",
    "phpexcel",
    "nova",
    "laravel-nova",
    "actions"
  ],
  "authors": [
    {
      "name": "Patrick Brouwers",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "require": {
    "php": "8.*",
    "laravel/nova": "*",
    "laravie/serialize-queries": "^1.0|^2.0",
    "neulabscom/excel": "4.1"
  },
  "autoload": {
    "psr-4": {
      "Maatwebsite\\LaravelNovaExcel\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/neulabscom/excel"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Maatwebsite\\LaravelNovaExcel\\LaravelNovaExcelServiceProvider"
      ]
    }
  }
}
